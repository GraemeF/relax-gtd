<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" x:Name="Inbox1"
	mc:Ignorable="d"
	x:Class="RelaxPrototypeScreens.Inbox">

	<DockPanel x:Name="LayoutRoot">
		<TextBlock Style="{DynamicResource BasicTextBlock-Sketch}" Text="Inbox" DockPanel.Dock="Top"/>
		<ListBox Style="{DynamicResource ListBox-Sketch}" DockPanel.Dock="Bottom">
			<ListBoxItem Style="{DynamicResource ListBoxItem-Sketch}" Content="Do Ironing"/>
			<ListBoxItem Style="{DynamicResource ListBoxItem-Sketch}" Content="Buy Christmans presents"/>
			<ListBoxItem Style="{DynamicResource ListBoxItem-Sketch}" Content="Sketch out design"/>
		</ListBox>
	</DockPanel>
</UserControl>